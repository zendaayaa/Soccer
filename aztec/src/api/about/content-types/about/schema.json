{
  "kind": "singleType",
  "collectionName": "abouts",
  "info": {
    "singularName": "about",
    "pluralName": "abouts",
    "displayName": "About",
    "description": "About page content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "default": "About Aztec Sports"
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mission": {
      "type": "text",
      "required": true
    },
    "vision": {
      "type": "text",
      "required": true
    },
    "values": {
      "type": "component",
      "repeatable": true,
      "component": "about.value"
    },
    "team": {
      "type": "component",
      "repeatable": true,
      "component": "about.team-member"
    },
    "stats": {
      "type": "component",
      "repeatable": true,
      "component": "about.stat"
    }
  }
}